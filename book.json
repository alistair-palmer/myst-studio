{
    "gitbook": "3.2.2",
    "plugins": [
        "hints",
        "simple-page-toc",
        "custom-favicon",
        "ga"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": false
        }, 
        "simple-page-toc": {
            "maxDepth": 7,
            "skipFirstH1": true
        },
        "favicon": "assets/favicon.ico",
        "disqus": {
            "shortName": "myst-studio-documentation"
        },
        "ga": {
            "token": "UA-35856338-5"
        }
    }
}